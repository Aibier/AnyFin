    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/config": "^0.0.35",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.144",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.11.7",
    "@types/supertest": "^2.0.8",
    "@types/webpack": "^4.39.5",
    "cross-env": "^6.0.3",
