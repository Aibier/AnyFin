{
  "name": "anyfin",
  "version": "1.0.0",
  "description": "Anyfin backend for creating http service using express written in typescript",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "start": "npm run dev",
    "prod": "cross-env NODE_ENV=production webpack --watch",
    "dev": " webpack --watch",
    "build": "npm run build:dev",
    "build:dev": "webpack --config ./webpack.config.js --progress --profile --color --display-error-details --display-cached && npm run copy-files",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --profile --color --display-error-details --display-cached --bail && npm run copy-files",
    "copy-files": "node build/copyFiles.js",
    "clean": "npm cache clear && rimraf -- dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "keywords": [],
  "author": "Tony",
  "license": "",
  "dependencies": {
    "@types/jest": "^24.0.23",
    "axios": "^0.21.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "config": "^3.2.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.3",
    "express-rate-limit": "^5.0.0",
    "express-status-monitor": "^1.2.7",
    "helmet": "^3.21.2",
    "i": "^0.3.7",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
